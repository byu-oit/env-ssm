{
  "name": "@byu-oit/env-ssm",
  "version": "3.0.0",
  "description": "Gets params from your environment first, then from the ssm parameter store.",
  "contributors": [
    "Gary Crye <gary_crye@byu.edu>",
    "Spencer Tuft <spencer.tuft@byu.edu>"
  ],
  "main": "dist/env-ssm.js",
  "types": "dist/env-ssm.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "del dist && tsc",
    "coverage": "npm run test -- --coverage || exit 0",
    "lint": "ts-standard | snazzy",
    "lint:fix": "ts-standard --fix | snazzy",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/byu-oit/env-ssm.git"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/byu-oit/env-ssm/issues"
  },
  "homepage": "https://github.com/byu-oit/env-ssm#readme",
  "dependencies": {
    "debug": "^4.3.1",
    "lodash.merge": "^4.6.2",
    "lodash.set": "^4.3.2"
  },
  "devDependencies": {
    "@aws-sdk/client-ssm": "^3.9.0",
    "@types/debug": "^4.1.5",
    "@types/jest": "^26.0.21",
    "@types/lodash.merge": "^4.6.6",
    "@types/lodash.set": "^4.3.6",
    "del-cli": "^3.0.1",
    "dotenv": "^8.2.0",
    "env-var": "^7.0.0",
    "lint-staged": "^10.5.4",
    "snazzy": "^9.0.0",
    "ts-jest": "^26.5.4",
    "ts-standard": "^10.0.0",
    "typescript": "^4.2.3"
  },
  "peerDependencies": {
    "@byu-oit/dottfvars": "^0.0.5",
    "dotenv": "^8.2.0",
    "env-var": "^7.0.0"
  },
  "optionalDependencies": {
    "@aws-sdk/client-ssm": ">=3"
  },
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  },
  "lint-staged": {
    "*.ts": "npm run lint:fix"
  }
}
